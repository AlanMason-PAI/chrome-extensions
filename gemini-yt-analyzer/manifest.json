{
  "manifest_version": 3,
  "name": "Analyze with Gemini",
  "version": "1.1.0",
  "description": "Quickly analyze YouTube videos with Google Gemini",
  "permissions": [
    "activeTab",
    "contextMenus",
    "scripting"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "Analyze with Gemini",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
